{
  "name": "wa_server",
  "version": "2.2.0",
  "description": "WhatsApp Integration Server",
  "scripts": {
    "update": "ncu -u",
    "lt": "lt --port 8000",
    "python:dev": "python3 -m venv ENV && ENV/bin/pip3 install -r requirements.txt && ENV/bin/fastapi dev src/routes/main.py",
    "python:prune": "rm -rf ENV && find . -name *.pyc -delete && find . -name __pycache__ -delete"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@openai/codex": "^0.47.0",
    "localtunnel": "^2.0.2",
    "npm-check-updates": "^19.1.1"
  }
}
